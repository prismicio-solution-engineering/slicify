      "uid": {
        "type": "UID",
        "config": {
          "label": "Slug",
          "placeholder": "the-page-url-slug"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "pricing": {
              "type": "SharedSlice"
            },
            "call_to_action": {
              "type": "SharedSlice"
            },
            "faq": {
              "type": "SharedSlice"
            },
            "logo_cloud": {
              "type": "SharedSlice"
            },
            "form": {
              "type": "SharedSlice"
            },
            "testimonials": {
              "type": "SharedSlice"
            },
            "features": {
              "type": "SharedSlice"
            },
            "hero": {
              "type": "SharedSlice"
            },
            "featured_websites_list": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO and Metadata": {
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "My title for Search Engine"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "The description that will appear in search engine"
        }
      },
      "canonicalLink": {
        "config": {
          "label": "CanonicalLink",
          "placeholder": "",
