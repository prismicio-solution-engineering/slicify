{
  "id": "blog_article",
  "label": "Blog Article",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Slug",
          "placeholder": "the-article-url-slug"
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "labels": [
            "highlight"
          ],
          "single": "heading1"
        }
      },
      "author": {
        "type": "Link",
        "config": {
          "label": "Author",
          "select": "document",
          "customtypes": [
            "author"
          ]
        }
      },
      "excerpt": {
        "type": "StructuredText",
        "config": {
          "label": "Excerpt",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "featured_image": {
        "type": "Image",
        "config": {
          "label": "Featured Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "category": {
        "type": "Link",
        "config": {
          "label": "Category",
          "select": "document",
          "customtypes": [
            "blog_category"
          ]
        }
      },
      "table_of_content": {
        "type": "Boolean",
        "config": {
          "label": "Table Of Content",
          "placeholder_false": "Hide",
          "placeholder_true": "Display",
          "default_value": true
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "text": {
              "type": "SharedSlice"
            },
            "form": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "Header": {},
    "Seo and metadata": {}
  }
}